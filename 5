using System;
using System.Collections.Generic;
using System.Linq;

public class Program
{
  public static void Main()
  {
    double a, b, c;
    Console.Write("Введите число a: ");
    a = double.Parse(Console.ReadLine());
    Console.Write("Введите число b: ");
    b = double.Parse(Console.ReadLine());
    c = Math.Abs(a);
    a = c;
    c = Math.Abs(b);
    b = c;
    c = a+b;
    Console.Write("Сумма модулей этих чисел равна ");
    Console.WriteLine(c);
    c = a-b;
    Console.WriteLine("Разность модулей этих чисел равна ");
    Console.WriteLine(c);
    c = a*b;
    Console.Write("Произведение модулей этих чисел равно ");
    Console.WriteLine(c);
    c = a/b;
    Console.Write("Частное модулей этих чисел равно ");
    Console.WriteLine(c);
  }
}
