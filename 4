using System;
using System.Collections.Generic;
using System.Linq;

public class Program
{
  public static void Main()
  {
    double a, b, c, q;
    Console.WriteLine("Введите число a: ");
    a = double.Parse(Console.ReadLine());
    Console.WriteLine("Введите число b: ");
    b = double.Parse(Console.ReadLine());
    q = a*a;
    a = q;
    q = b*b;
    b = q;
    c = a+b;
    Console.Write("Сумма квадратов этих чисел равна ");
    Console.WriteLine(c);
    c = a-b;
    Console.Write("Разность квадратов этих чисел равна ");
    Console.WriteLine(c);
    c = a*b;
    Console.Write("Произведение квадратов этих чисел равно ");
    Console.WriteLine(c);
    c = a/b;
    Console.Write("Частное квадратов этих чисел равно ");
    Console.WriteLine(c);
  }
}
